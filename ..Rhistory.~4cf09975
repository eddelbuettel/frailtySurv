set.seed(2015)
dat <- genfrail(N=300, K=2, beta=c(log(2),log(3)),
frailty="gamma", theta=2,
lambda_0=function(t, tau=4.6, C=0.01) (tau*(C*t)^tau)/t)
fit <- fitfrail(Surv(time, status) ~ Z1 + Z2 + cluster(family),
dat, frailty="gamma", fitmethod="loglik")
COV.est <- vcov(fit)
COV.est
sqrt(diag(COV.est))
