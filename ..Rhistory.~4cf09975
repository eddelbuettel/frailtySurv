set.seed(1234)
dat <- genfrail(N=30, K=2, beta=c(log(2),log(3)), theta=2, frailty="gamma")
fit <- fitfrail(Surv(time, status) ~ Z1 + Z2 + cluster(family), dat, frailty="gamma")
fit <- fitfrail(Surv(time, status) ~ Z1 + Z2 + cluster(family), dat, frailty="gamma", verbose=TRUE)
fit <- fitfrail(Surv(time, status) ~ Z1 + Z2 + cluster(family), dat, frailty="gamma", verbose=TRUE, se=TRUE)
fit <- fitfrail(Surv(time, status) ~ Z1 + Z2 + cluster(family), dat, frailty="gamma", verbose=TRUE, se=TRUE, fitmethod="score")
